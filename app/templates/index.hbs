{{outlet}}
{{#if session.isAuthenticated}}
<ul>
  <li>
    {{ link-to 'New Conversation' 'conversations.new' }}
  </li>
</ul>
<h4>My Conversations</h4>
<ul>
{{#each sessionUser.user.conversations as |convo|}}
  <li>{{link-to convo.code 'conversations.conversation' convo}}</li>
{{/each}}
</ul>
{{/if}}

